  "patient_reports": [
    {
      "form": "tb_screening",
      "events": [
        {
          "name": "on_create",
          "trigger": "assign_schedule",
          "params": "Presumptive TB alert",
          "bool_expr": "doc.fields.c_refer_for_sputum === 'yes'"
        }
      ]
    },
    {
      "form": "trace",
      "events": [
        {
          "name": "on_create",
          "trigger": "assign_schedule",
          "params": "Trace report alert",
          "bool_expr": "doc.fields.trace_details.trace_reasons !== ''"
        }
      ]
    },
    {
      "form": "tb_results",
      "events": [
        {
          "name": "on_create",
          "trigger": "assign_schedule",
          "params": "TB results alert",
          "bool_expr": "doc.fields.tb_results.results !== 'rejected'"
        }
      ]
    },
    {
      "form": "discharge",
      "events": [
        {
          "name": "on_create",
          "trigger": "assign_schedule",
          "params": "Discharge report alert",
          "bool_expr": "doc.fields.discharge_details.discharge_diagnoses && /yes/.test(doc.fields.discharge_details.discharge_diagnoses)"
        }
      ]
    },
    {
      "form": "tb_results",
      "events": [
        {
          "name": "on_create",
          "trigger": "assign_schedule",
          "params": "Rejected TB sample alert",
          "bool_expr": "doc.fields.tb_results.results === 'rejected'"
        }
      ]
    }
  ],
