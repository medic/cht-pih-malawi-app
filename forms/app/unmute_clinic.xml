      <bind nodeset="/unmute_clinic/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/unmute_clinic/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/unmute_clinic/inputs/contact/sex" type="string"/>
      <bind nodeset="/unmute_clinic/inputs/contact/parent/parent/contact/name" type="string"/>
      <bind nodeset="/unmute_clinic/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind calculate="../inputs/source" nodeset="/unmute_clinic/source" type="string"/>
      <bind calculate="../inputs/source_id" nodeset="/unmute_clinic/source_id" type="string"/>
      <bind calculate="../inputs/contact/_id" nodeset="/unmute_clinic/place_id" type="string"/>
      <bind calculate="../inputs/contact/name" nodeset="/unmute_clinic/place_name" type="string"/>
      <bind calculate="if(string-length( /unmute_clinic/unmute_request/reason ) !=0,jr:choice-name( /unmute_clinic/unmute_request/reason , ' /unmute_clinic/unmute_request/reason '), 'None')" nodeset="/unmute_clinic/unmute_choice" type="string"/>
      <bind calculate="../inputs/contact/contact/name" nodeset="/unmute_clinic/hh_contact" type="string"/>
      <bind calculate="concat(../../inputs/meta/location/lat, concat(' ', ../../inputs/meta/location/long))" nodeset="/unmute_clinic/geolocation" type="string"/>
      <bind nodeset="/unmute_clinic/unmute_request/reason" required="true()" type="select1"/>
      <bind nodeset="/unmute_clinic/unmute_request/reason_other" relevant="selected(../reason, 'other')" required="true()" type="string"/>
      <bind nodeset="/unmute_clinic/group_review/n_submit" readonly="true()" type="string"/>
      <bind nodeset="/unmute_clinic/group_review/n_title" readonly="true()" type="string"/>
      <bind nodeset="/unmute_clinic/group_review/n_name" readonly="true()" type="string"/>
      <bind nodeset="/unmute_clinic/group_review/n_contact" readonly="true()" type="string"/>
      <bind nodeset="/unmute_clinic/group_review/n_reason" readonly="true()" type="string"/>
      <bind nodeset="/unmute_clinic/group_review/n_reason_mute" readonly="true()" relevant="not(selected(../../unmute_request/reason, 'other'))" type="string"/>
      <bind nodeset="/unmute_clinic/group_review/n_reason_mute_other" readonly="true()" relevant="selected(../../unmute_request/reason, 'other')" type="string"/>
      <bind nodeset="/unmute_clinic/group_review/n_muting_activated" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/unmute_clinic/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/unmute_clinic/inputs">
      <label ref="jr:itext('/unmute_clinic/inputs:label')"/>
      <group ref="/unmute_clinic/inputs/contact">
        <label ref="jr:itext('/unmute_clinic/inputs/contact:label')"/>
