              <label ref="jr:itext('/unmute_clinic/inputs/contact/parent/parent/contact:label')"/>
            </group>
          </group>
        </group>
      </group>
    </group>
    <group appearance="field-list" ref="/unmute_clinic/unmute_request">
      <label ref="jr:itext('/unmute_clinic/unmute_request:label')"/>
      <select1 ref="/unmute_clinic/unmute_request/reason">
        <label ref="jr:itext('/unmute_clinic/unmute_request/reason:label')"/>
        <item>
          <label ref="jr:itext('/unmute_clinic/unmute_request/reason/accepted_service:label')"/>
          <value>accepted_service</value>
        </item>
        <item>
          <label ref="jr:itext('/unmute_clinic/unmute_request/reason/moved_back:label')"/>
          <value>moved_back</value>
        </item>
        <item>
          <label ref="jr:itext('/unmute_clinic/unmute_request/reason/other:label')"/>
          <value>other</value>
        </item>
      </select1>
      <input ref="/unmute_clinic/unmute_request/reason_other">
        <label ref="jr:itext('/unmute_clinic/unmute_request/reason_other:label')"/>
      </input>
    </group>
    <group appearance="field-list summary" ref="/unmute_clinic/group_review">
      <label ref="jr:itext('/unmute_clinic/group_review:label')"/>
      <input ref="/unmute_clinic/group_review/n_submit">
        <label ref="jr:itext('/unmute_clinic/group_review/n_submit:label')"/>
      </input>
      <input appearance="h1 yellow" ref="/unmute_clinic/group_review/n_title">
        <label ref="jr:itext('/unmute_clinic/group_review/n_title:label')"/>
      </input>
      <input appearance="h1" ref="/unmute_clinic/group_review/n_name">
        <label ref="jr:itext('/unmute_clinic/group_review/n_name:label')"/>
      </input>
      <input ref="/unmute_clinic/group_review/n_contact">
        <label ref="jr:itext('/unmute_clinic/group_review/n_contact:label')"/>
      </input>
      <input appearance="h1 blue" ref="/unmute_clinic/group_review/n_reason">
        <label ref="jr:itext('/unmute_clinic/group_review/n_reason:label')"/>
      </input>
      <input appearance="li" ref="/unmute_clinic/group_review/n_reason_mute">
        <label ref="jr:itext('/unmute_clinic/group_review/n_reason_mute:label')"/>
      </input>
      <input appearance="li" ref="/unmute_clinic/group_review/n_reason_mute_other">
        <label ref="jr:itext('/unmute_clinic/group_review/n_reason_mute_other:label')"/>
      </input>
      <input appearance="li" ref="/unmute_clinic/group_review/n_muting_activated">
        <label ref="jr:itext('/unmute_clinic/group_review/n_muting_activated:label')"/>
      </input>
    </group>
