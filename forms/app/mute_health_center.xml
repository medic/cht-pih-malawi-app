      <bind calculate="../inputs/contact/contact/name" nodeset="/mute_health_center/chw_contact_name" type="string"/>
      <bind calculate="../inputs/contact/contact/name" nodeset="/mute_health_center/geolocation" type="string"/>
      <bind nodeset="/mute_health_center/mute_request/reason" required="true()" type="select1"/>
      <bind nodeset="/mute_health_center/mute_request/reason_other" relevant="selected(../reason, 'other')" required="true()" type="string"/>
      <bind nodeset="/mute_health_center/group_review/n_submit" readonly="true()" type="string"/>
      <bind nodeset="/mute_health_center/group_review/n_title" readonly="true()" type="string"/>
      <bind nodeset="/mute_health_center/group_review/n_name" readonly="true()" type="string"/>
      <bind nodeset="/mute_health_center/group_review/n_contact" readonly="true()" type="string"/>
      <bind nodeset="/mute_health_center/group_review/n_reason" readonly="true()" type="string"/>
      <bind nodeset="/mute_health_center/group_review/n_reason_mute" readonly="true()" relevant="not(selected(../../mute_request/reason, 'other')) and count-selected(../../mute_request/reason) &gt; 0" type="string"/>
      <bind nodeset="/mute_health_center/group_review/n_reason_mute_other" readonly="true()" relevant="selected(../../mute_request/reason, 'other')" type="string"/>
      <bind nodeset="/mute_health_center/group_review/n_muting_activated" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/mute_health_center/meta/instanceID" readonly="true()" type="string"/>
    </model>
