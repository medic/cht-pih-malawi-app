      <bind calculate="../patient_age_in_years" nodeset="/sputum_collection/c_patient_age_in_years" type="string"/>
      <bind calculate="difference-in-months(  /sputum_collection/patient_date_of_birth , today() )" nodeset="/sputum_collection/patient_age_in_months" type="string"/>
      <bind calculate="floor( decimal-date-time(today()) - decimal-date-time( /sputum_collection/patient_date_of_birth ) )" nodeset="/sputum_collection/patient_age_in_days" type="string"/>
      <bind calculate="1" nodeset="/sputum_collection/samples_count" type="string"/>
      <bind calculate="../inputs/contact/date_of_birth" nodeset="/sputum_collection/date_of_birth" type="string"/>
      <bind calculate="../inputs/contact/parent/contact/phone" nodeset="/sputum_collection/chw_phone" type="string"/>
      <bind calculate="concat(../../inputs/meta/location/lat, concat(' ', ../../inputs/meta/location/long))" nodeset="/sputum_collection/geolocation" type="string"/>
      <bind calculate="true()" nodeset="/sputum_collection/needs_signoff" type="string"/>
      <bind nodeset="/sputum_collection/sputum_collection/samples_collected" required="true()" type="select1"/>
      <bind nodeset="/sputum_collection/sputum_collection/reasons_no_sample_collected" relevant="selected(../samples_collected, '0')" required="true()" type="select1"/>
      <bind nodeset="/sputum_collection/sputum_collection/reasons_no_sample_collected_other" relevant="selected(../reasons_no_sample_collected, 'other')" type="string"/>
      <bind nodeset="/sputum_collection/sputum_collection/n_refer" readonly="true()" relevant="selected(../reasons_no_sample_collected, 'no_sample_with_tb_symptoms') and not(selected(../reasons_no_sample_collected, 'other'))" type="string"/>
      <bind nodeset="/sputum_collection/sputum_collection/n_inform" readonly="true()" relevant="selected(../reasons_no_sample_collected, 'no_tb_symptoms_confirmed') and not(selected(../reasons_no_sample_collected, 'other'))" type="string"/>
      <bind constraint=". &lt;=  /sputum_collection/sputum_collection/samples_collected " jr:constraintMsg="jr:itext('/sputum_collection/sputum_collection/samples_dropped_at_facility:jr:constraintMsg')" nodeset="/sputum_collection/sputum_collection/samples_dropped_at_facility" relevant="not(selected(../samples_collected, '0'))" required="true()" type="select1"/>
      <bind nodeset="/sputum_collection/sputum_collection/reason_no_sample_dropped" relevant=" /sputum_collection/sputum_collection/samples_dropped_at_facility  !='' and  /sputum_collection/sputum_collection/samples_dropped_at_facility  &lt;  /sputum_collection/sputum_collection/samples_collected " required="true()" type="string"/>
      <bind nodeset="/sputum_collection/group_review/n_summary_title" readonly="true()" type="string"/>
      <bind nodeset="/sputum_collection/group_review/n_submit" readonly="true()" type="string"/>
      <bind nodeset="/sputum_collection/group_review/n_patient_details_title" readonly="true()" relevant="../../inputs/contact/patient_id != ''" type="string"/>
      <bind nodeset="/sputum_collection/group_review/n_patient_details" readonly="true()" relevant="../../inputs/contact/patient_id != ''" type="string"/>
      <bind nodeset="/sputum_collection/group_review/n_findings_title" readonly="true()" relevant=" /sputum_collection/sputum_collection/samples_collected  != ''" type="string"/>
      <bind nodeset="/sputum_collection/group_review/n_samples_collected" readonly="true()" relevant=" /sputum_collection/sputum_collection/samples_collected  != ''" type="string"/>
      <bind nodeset="/sputum_collection/group_review/n_samples_submitted" readonly="true()" relevant=" /sputum_collection/sputum_collection/samples_collected  != ''" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/sputum_collection/meta/instanceID" readonly="true()" type="string"/>
