      <select1 ref="/fp_follow_up/fp_follow_up/using_condoms">
        <label ref="jr:itext('/fp_follow_up/fp_follow_up/using_condoms:label')"/>
        <item>
          <label ref="jr:itext('/fp_follow_up/fp_follow_up/using_condoms/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/fp_follow_up/fp_follow_up/using_condoms/no:label')"/>
          <value>no</value>
        </item>
      </select1>
      <input ref="/fp_follow_up/fp_follow_up/n_no_condoms">
