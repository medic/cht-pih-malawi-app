      <bind calculate="../source_id" nodeset="/fp_follow_up/source_uuid" type="string"/>
      <bind calculate="fp'" nodeset="/fp_follow_up/fu_type" type="string"/>
      <bind calculate="../side_effects" nodeset="/fp_follow_up/side_effects" type="string"/>
      <bind calculate="if(../new_fp_method !='', 'yes', 'no')" nodeset="/fp_follow_up/gave_fp_method" type="string"/>
      <bind calculate="../new_fp_method" nodeset="/fp_follow_up/gave_fp_method_type" type="string"/>
      <bind calculate="../inputs/contact/date_of_birth" nodeset="/fp_follow_up/date_of_birth" type="string"/>
      <bind calculate="../inputs/contact/parent/contact/phone" nodeset="/fp_follow_up/chw_phone" type="string"/>
      <bind calculate="concat(../../inputs/meta/location/lat, concat(' ', ../../inputs/meta/location/long))" nodeset="/fp_follow_up/geolocation" type="string"/>
      <bind calculate="if( /fp_follow_up/fp_method  = 'pills', 'Pills', if( /fp_follow_up/fp_method  = 'iucd', 'Intrauterine Device (IUCD)', if( /fp_follow_up/fp_method  = 'implant', 'Implant', if( /fp_follow_up/fp_method  = 'tubal_ligation', 'Tubal Ligation', if( /fp_follow_up/fp_method  = 'depo_provera', 'Injection/Depo-Provera')))))" nodeset="/fp_follow_up/fp_follow_up/fp_method_display" type="string"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/on_fp" required="true()" type="select1"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/n_fp_method" readonly="true()" relevant="selected(../on_fp, 'yes')" type="string"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/changed_fp_method" relevant="../../fp_method != 'tubal_ligation'" type="select1"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/new_fp_method" relevant="selected(../changed_fp_method, 'yes')" type="select1"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/expiry_date" relevant="(selected(../changed_fp_method, 'no') and (../../fp_method = 'implant' or ../../fp_method = 'iucd' or ../../fp_method = 'depo_provera' )) or (selected(../changed_fp_method, 'yes') and (../new_fp_method = 'implant' or ../new_fp_method = 'iucd' or ../new_fp_method = 'depo_provera'))" required="false()" type="date"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/n_ref_for_renewal" readonly="true()" relevant="decimal-date-time(../expiry_date) &lt;= decimal-date-time(today())" type="string"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/using_condoms" relevant="../../fp_method = 'implant' or ../new_fp_method = 'implant'
and ../../is_in_art = 'yes'" type="select1"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/n_no_condoms" readonly="true()" relevant="selected(../using_condoms, 'no')" type="string"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/side_effects" relevant="selected(../on_fp, 'yes')" required="true()" type="select1"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/n_cousel_and_ref" readonly="true()" relevant="selected(../side_effects, 'yes')" type="string"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/reasons_for_no_fp" relevant="selected(../on_fp, 'no')" required="true()" type="select"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/reasons_for_no_fp_other" relevant="selected(../reasons_for_no_fp, 'other')" required="true()" type="string"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/wants_to_change_fp" relevant="selected(../on_fp, 'yes') and ../../fp_method != 'tubal_ligation' and ../new_fp_method != 'tubal_ligation'" type="select1"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/has_appointment_date" required="true()" type="select1"/>
      <bind constraint="decimal-date-time(.) &gt;= decimal-date-time(today())" jr:constraintMsg="jr:itext('/fp_follow_up/fp_follow_up/next_visit:jr:constraintMsg')" nodeset="/fp_follow_up/fp_follow_up/next_visit" relevant="selected(../has_appointment_date, 'yes')" required="true()" type="date"/>
      <bind calculate="format-date-time(../next_visit,&quot;%d-%m-%Y&quot;)" nodeset="/fp_follow_up/fp_follow_up/fp_next_visit" relevant="../next_visit != ''" type="string"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/n_next_visit" readonly="true()" relevant="../next_visit != ''" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/fp_follow_up/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/fp_follow_up/inputs">
      <label ref="jr:itext('/fp_follow_up/inputs:label')"/>
