      <bind nodeset="/death/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/death/inputs/source" type="string"/>
      <bind nodeset="/death/inputs/source_id" type="string"/>
      <bind nodeset="/death/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/death/inputs/contact/name" type="string"/>
      <bind nodeset="/death/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/death/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/death/inputs/contact/sex" type="string"/>
      <bind nodeset="/death/inputs/contact/parent/parent/contact/name" type="string"/>
      <bind nodeset="/death/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind calculate="../inputs/source" nodeset="/death/source" type="string"/>
      <bind calculate="../inputs/source_id" nodeset="/death/source_id" type="string"/>
      <bind calculate="floor( difference-in-months( ../inputs/contact/date_of_birth, today() ) div 12 )" nodeset="/death/patient_age_in_years" type="string"/>
      <bind calculate="difference-in-months(  /death/inputs/contact/date_of_birth , today() )" nodeset="/death/patient_age_in_months" type="string"/>
      <bind calculate="floor( decimal-date-time(today()) - decimal-date-time( /death/inputs/contact/date_of_birth ) )" nodeset="/death/patient_age_in_days" type="string"/>
      <bind calculate="floor((decimal-date-time(today()) - decimal-date-time( /death/inputs/contact/date_of_birth )) div 7)" nodeset="/death/patient_age_in_weeks" type="string"/>
      <bind calculate="../inputs/contact/_id" nodeset="/death/patient_uuid" type="string"/>
      <bind calculate="../inputs/contact/_id" nodeset="/death/patient_id" required="true()" type="string"/>
      <bind calculate="../inputs/contact/name" nodeset="/death/patient_name" type="string"/>
      <bind calculate="../death_details/death_date" nodeset="/death/death_date" type="string"/>
      <bind calculate="../death_date" nodeset="/death/date_of_death" type="string"/>
      <bind calculate="../inputs/contact/parent/contact/phone" nodeset="/death/chw_phone" type="string"/>
      <bind calculate="concat(../../inputs/meta/location/lat, concat(' ', ../../inputs/meta/location/long))" nodeset="/death/geolocation" type="string"/>
      <bind calculate="instance('contact-summary')/context/is_in_art" nodeset="/death/is_in_art" type="string"/>
      <bind calculate="instance('contact-summary')/context/is_in_tb" nodeset="/death/is_in_tb" type="string"/>
      <bind calculate="instance('contact-summary')/context/is_in_malnutrition" nodeset="/death/is_in_malnutrition" type="string"/>
      <bind calculate="instance('contact-summary')/context/is_in_fp" nodeset="/death/is_in_fp" type="string"/>
      <bind calculate="round(../c_art_duration div 365, 0)" nodeset="/death/art_duration" type="string"/>
      <bind calculate="round(../c_tb_treatment_duration div 365, 0)" nodeset="/death/tb_treatment_duration" type="string"/>
      <bind calculate="instance('contact-summary')/context/is_in_ncd" nodeset="/death/is_in_ncd" type="string"/>
      <bind calculate="1" nodeset="/death/is_in_others_group" type="string"/>
      <bind calculate="if(../is_in_art = 'yes', 1, 0)" nodeset="/death/is_in_art_group" type="string"/>
      <bind calculate="if(../is_in_tb = 'yes', 1, 0)" nodeset="/death/is_in_tb_group" type="string"/>
      <bind calculate="if(../is_in_ncd = 'yes', 1, 0)" nodeset="/death/is_in_ncd_group" type="string"/>
      <bind calculate="if(../is_in_malnutrition = 'yes', 1, 0)" nodeset="/death/is_in_malnutrition_group" type="string"/>
      <bind constraint="decimal-date-time(.) &lt;= decimal-date-time(today())" jr:constraintMsg="jr:itext('/death/death_details/death_date:jr:constraintMsg')" nodeset="/death/death_details/death_date" required="true()" type="date"/>
      <bind constraint="not(selected(., 'none')
and (count-selected(.) &gt; 1))" jr:constraintMsg="jr:itext('/death/death_details/treatment_program:jr:constraintMsg')" nodeset="/death/death_details/treatment_program" required="true()" type="select"/>
      <bind nodeset="/death/death_details/treatment_program_other" relevant="selected(../treatment_program, 'other')" required="true()" type="string"/>
      <bind nodeset="/death/death_details/death_place" required="true()" type="select1"/>
      <bind nodeset="/death/group_review/n_submit" readonly="true()" type="string"/>
      <bind nodeset="/death/group_review/n_patient_details_title" readonly="true()" type="string"/>
      <bind calculate="../../patient_age_in_months mod 12" nodeset="/death/group_review/c_patient_mod_mnths" type="string"/>
      <bind calculate="if(../../patient_age_in_years &lt;= 5, 
concat( /death/patient_age_in_years , ' years ',  /death/group_review/c_patient_mod_mnths , ' months old'),
concat( /death/patient_age_in_years , ' years old'))" nodeset="/death/group_review/c_age_string" type="string"/>
      <bind nodeset="/death/group_review/n_patient_details" readonly="true()" type="string"/>
      <bind nodeset="/death/group_review/n_treatment_program_title" readonly="true()" type="string"/>
      <bind nodeset="/death/group_review/n_art" readonly="true()" relevant="selected(../../death_details/treatment_program, 'art')" type="string"/>
      <bind nodeset="/death/group_review/n_tb" readonly="true()" relevant="selected(../../death_details/treatment_program, 'tb')" type="string"/>
      <bind nodeset="/death/group_review/n_malnutrition" readonly="true()" relevant="selected(../../death_details/treatment_program, 'malnutrition')" type="string"/>
      <bind nodeset="/death/group_review/n_ncd" readonly="true()" relevant="selected(../../death_details/treatment_program, 'ncd')" type="string"/>
      <bind nodeset="/death/group_review/n_other" readonly="true()" relevant="selected(../../death_details/treatment_program, 'other')" type="string"/>
      <bind nodeset="/death/group_review/n_none" readonly="true()" relevant="selected(../../death_details/treatment_program, 'none')" type="string"/>
      <bind calculate="format-date(../../death_date, &quot;%d/%m/%Y&quot;)" nodeset="/death/group_review/c_dod" type="string"/>
      <bind nodeset="/death/group_review/n_date" readonly="true()" type="string"/>
      <bind nodeset="/death/group_review/n_at_hf" readonly="true()" relevant="selected(../../death_details//death_place, 'health_facility')" type="string"/>
      <bind nodeset="/death/group_review/n_outside_hf" readonly="true()" relevant="selected(../../death_details//death_place, 'outside_health_facility')" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/death/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
