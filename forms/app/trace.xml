      <bind calculate="if(selected(../trace_reasons, 'art'), 'ART missed visit','')" nodeset="/trace/trace_details/c_trace_art" type="string"/>
      <bind calculate="if(selected(../trace_reasons, 'tb'), 'TB missed visit','')" nodeset="/trace/trace_details/c_trace_tb" type="string"/>
      <bind calculate="if(selected(../trace_reasons, 'malnutrition'), 'Malnutrition missed visit','')" nodeset="/trace/trace_details/c_trace_malnutrition" type="string"/>
      <bind calculate="if(selected(../trace_reasons, 'lab_results'), 'Lab results ready','')" nodeset="/trace/trace_details/c_trace_lab_results" type="string"/>
      <bind calculate="if(selected(../trace_reasons, 'due_for_lab'), 'Due for lab work','')" nodeset="/trace/trace_details/c_trace_due_for_lab" type="string"/>
