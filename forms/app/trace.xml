      <bind calculate="if(selected(../trace_reasons, 'other'), 'Other','')" nodeset="/trace/trace_details/c_trace_other" type="string"/>
      <bind calculate="if(selected(../trace_reasons, 'ncd') or selected(../trace_reasons, 'ncd'), 'IC3 clinic missed visit','')" nodeset="/trace/trace_details/c_trace_ic3" type="string"/>
      <bind calculate="concat( /trace/trace_details/c_trace_ic3 , ', ',  /trace/trace_details/c_trace_tb , ', ',  /trace/trace_details/c_trace_malnutrition , ', ',  /trace/trace_details/c_trace_lab_results , ', ',  /trace/trace_details/c_trace_due_for_lab , ', ',  /trace/trace_details/c_trace_linkage_to_care , ', ',  /trace/trace_details/c_trace_other )" nodeset="/trace/trace_details/c_trace_reasons" type="string"/>
