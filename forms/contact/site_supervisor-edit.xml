      <bind calculate="../../../inputs/user/facility_id" nodeset="/data/site_supervisor/meta/last_edited_by_place_uuid" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
