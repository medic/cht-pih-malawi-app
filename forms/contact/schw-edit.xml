      <bind nodeset="/data/inputs" relevant="0"/>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind calculate="../inputs/user/facility_id" nodeset="/data/facility_id" type="string"/>
      <bind nodeset="/data/n_none_hh_member" readonly="true()" relevant="starts-with( /data/chw/parent , ../facility_id)" type="string"/>
      <bind calculate="if(starts-with( /data/chw/parent , ../facility_id),1,0)" nodeset="/data/none_hh_member_flag" type="string"/>
      <bind nodeset="/data/chw" relevant=" /data/none_hh_member_flag  = 0"/>
      <bind nodeset="/data/chw/type" type="string"/>
      <bind nodeset="/data/chw/contact_type" type="string"/>
      <bind nodeset="/data/chw/parent" type="string"/>
      <bind constraint="regex(.,'[a-zA-Z]{1,50}[ ]{0,1}[a-zA-Z]{1,50}')" jr:constraintMsg="jr:itext('/data/chw/name:jr:constraintMsg')" nodeset="/data/chw/name" required="true()" type="string"/>
      <bind nodeset="/data/chw/sex" required="true()" type="select1"/>
      <bind calculate="format-date-time(date(../ephemeral_dob/dob_iso),'%Y-%m-%d')" nodeset="/data/chw/date_of_birth" type="string"/>
      <bind nodeset="/data/chw/ephemeral_dob/contact_dob_method" type="select1"/>
      <bind jr:constraintMsg="jr:itext('/data/chw/ephemeral_dob/dob_calendar:jr:constraintMsg')" nodeset="/data/chw/ephemeral_dob/dob_calendar" relevant="selected(../contact_dob_method,'calendar')" required="true()" type="date"/>
      <bind jr:constraintMsg="jr:itext('/data/chw/ephemeral_dob/age:jr:constraintMsg')" nodeset="/data/chw/ephemeral_dob/age" relevant="selected(../contact_dob_method,'approx')" required="true()" type="int"/>
      <bind calculate="if(selected( ../contact_dob_method,'calendar'), ../dob_calendar, format-date-time( decimal-date-time( today() ) - (365.25 * ../age) , &quot;%Y-%m-%d&quot;) )" nodeset="/data/chw/ephemeral_dob/dob_raw" type="string"/>
      <bind calculate="decimal-date-time(../dob_raw)" nodeset="/data/chw/ephemeral_dob/dob_iso" type="string"/>
      <bind jr:constraintMsg="jr:itext('/data/chw/phone:jr:constraintMsg')" nodeset="/data/chw/phone" required="false()" type="string"/>
      <bind jr:constraintMsg="jr:itext('/data/chw/alternate_phone:jr:constraintMsg')" nodeset="/data/chw/alternate_phone" required="false()" type="string"/>
      <bind nodeset="/data/chw/treatment_program" type="string"/>
      <bind nodeset="/data/chw/education_level" type="select1"/>
      <bind nodeset="/data/chw/date_joined" type="date"/>
      <bind nodeset="/data/chw/notes" type="string"/>
      <bind nodeset="/data/chw/meta/created_by" type="string"/>
      <bind nodeset="/data/chw/meta/created_by_person_uuid" type="string"/>
      <bind nodeset="/data/chw/meta/created_by_place_uuid" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group ref="/data/inputs">
      <group ref="/data/inputs/user">
        <input ref="/data/inputs/user/contact_id">
          <label ref="jr:itext('/data/inputs/user/contact_id:label')"/>
        </input>
        <input ref="/data/inputs/user/facility_id">
          <label ref="jr:itext('/data/inputs/user/facility_id:label')"/>
