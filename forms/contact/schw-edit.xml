      <instance>
        <data delimiter="#" id="contact:schw:edit" prefix="J1!contact:schw:edit!" version="2021-08-06 19:26:56">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <user>
              <contact_id/>
              <facility_id/>
              <name/>
            </user>
          </inputs>
          <facility_id/>
          <n_none_hh_member/>
          <none_hh_member_flag/>
          <chw>
            <type>contact</type>
            <contact_type>chw</contact_type>
            <parent>PARENT</parent>
            <name/>
            <sex/>
            <date_of_birth/>
            <ephemeral_dob>
              <contact_dob_method/>
              <dob_calendar/>
              <age/>
              <dob_raw/>
              <dob_iso/>
            </ephemeral_dob>
            <phone/>
            <alternate_phone/>
            <treatment_program/>
            <education_level/>
            <date_joined/>
            <notes/>
            <meta tag="hidden">
              <created_by/>
              <created_by_person_uuid/>
              <created_by_place_uuid/>
            </meta>
          </chw>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </data>
