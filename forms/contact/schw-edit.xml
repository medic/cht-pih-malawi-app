      </group>
    </group>
    <input ref="/data/n_none_hh_member">
      <label ref="jr:itext('/data/n_none_hh_member:label')"/>
    </input>
    <group appearance="field-list" ref="/data/chw">
      <label ref="jr:itext('/data/chw:label')"/>
      <input ref="/data/chw/name">
        <label ref="jr:itext('/data/chw/name:label')"/>
      </input>
      <select1 ref="/data/chw/sex">
        <label ref="jr:itext('/data/chw/sex:label')"/>
        <item>
          <label ref="jr:itext('/data/chw/sex/male:label')"/>
          <value>male</value>
        </item>
        <item>
          <label ref="jr:itext('/data/chw/sex/female:label')"/>
          <value>female</value>
        </item>
      </select1>
      <group ref="/data/chw/ephemeral_dob">
        <select1 ref="/data/chw/ephemeral_dob/contact_dob_method">
          <label ref="jr:itext('/data/chw/ephemeral_dob/contact_dob_method:label')"/>
          <item>
            <label ref="jr:itext('/data/chw/ephemeral_dob/contact_dob_method/approx:label')"/>
            <value>approx</value>
          </item>
          <item>
            <label ref="jr:itext('/data/chw/ephemeral_dob/contact_dob_method/calendar:label')"/>
            <value>calendar</value>
          </item>
        </select1>
        <input ref="/data/chw/ephemeral_dob/dob_calendar">
          <label ref="jr:itext('/data/chw/ephemeral_dob/dob_calendar:label')"/>
        </input>
        <input ref="/data/chw/ephemeral_dob/age">
          <label ref="jr:itext('/data/chw/ephemeral_dob/age:label')"/>
        </input>
      </group>
      <input appearance="numbers" ref="/data/chw/phone">
        <label ref="jr:itext('/data/chw/phone:label')"/>
        <hint ref="jr:itext('/data/chw/phone:hint')"/>
      </input>
      <input appearance="numbers" ref="/data/chw/alternate_phone">
        <label ref="jr:itext('/data/chw/alternate_phone:label')"/>
        <hint ref="jr:itext('/data/chw/alternate_phone:hint')"/>
      </input>
      <input appearance="hidden" ref="/data/chw/treatment_program">
        <label ref="jr:itext('/data/chw/treatment_program:label')"/>
      </input>
      <select1 ref="/data/chw/education_level">
        <label ref="jr:itext('/data/chw/education_level:label')"/>
        <item>
          <label ref="jr:itext('/data/chw/education_level/none:label')"/>
          <value>none</value>
        </item>
        <item>
          <label ref="jr:itext('/data/chw/education_level/primary:label')"/>
          <value>primary</value>
        </item>
        <item>
          <label ref="jr:itext('/data/chw/education_level/secondary:label')"/>
          <value>secondary</value>
        </item>
        <item>
          <label ref="jr:itext('/data/chw/education_level/college:label')"/>
          <value>college</value>
        </item>
        <item>
          <label ref="jr:itext('/data/chw/education_level/university:label')"/>
          <value>university</value>
        </item>
      </select1>
      <input ref="/data/chw/date_joined">
        <label ref="jr:itext('/data/chw/date_joined:label')"/>
      </input>
      <input ref="/data/chw/notes">
        <label ref="jr:itext('/data/chw/notes:label')"/>
      </input>
      <group ref="/data/chw/meta"/>
    </group>
  </h:body>
</h:html>
